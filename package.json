{
  "name": "medten-deepdive",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "VITE_MOCK_MODE=false vite",
    "dev:mock": "vite",
    "dev:live": "VITE_MOCK_MODE=false vite",
    "posthog:dev": "node server/posthog-dev.js",
    "build": "vite build",
    "build:live": "VITE_MOCK_MODE=false vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "deploy": "npm run build:live && aws s3 sync dist/ s3://medten-deepdive --delete",
    "deploy:prod": "npm run build:live && aws s3 sync dist/ s3://medten-deepdive --delete --profile production"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "@tailwindcss/postcss": "^4.1.12",
    "axios": "^1.7.7",
    "chart.js": "^4.5.0",
    "lottie-web": "^5.13.0",
    "pinia": "^3.0.3",
    "posthog-js": "^1.260.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.12",
    "vite": "^5.1.4",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4"
  }
}
